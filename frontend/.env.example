# Environment variables for frontend deployment
# Copy this to .env.local for local development
# Add these to Vercel dashboard for production

# API URL - point to your Render backend
NEXT_PUBLIC_API_URL=https://googleadsdashboard.onrender.com/api
NEXT_PUBLIC_APP_NAME=TellSpike
NEXT_PUBLIC_APP_ENV=production

# Kelkoo API (server-side only)
KELKOO_API_TOKEN=your_kelkoo_bearer_token

# Admedia API (server-side only)
ADMEDIA_AID=your_admedia_aid
ADMEDIA_API_KEY=your_admedia_api_key

# MaxBounty API (server-side only)
MAXBOUNTY_EMAIL=your_maxbounty_email
MAXBOUNTY_PASSWORD=your_maxbounty_password
